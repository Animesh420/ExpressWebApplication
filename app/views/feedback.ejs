<!DOCTYPE html>
<html>
  <head>
   <% include partials/template/head.ejs %>
  </head>
  <body id="<%= pageID %>">
<% include partials/template/header.ejs %>
    <div class="container">
      <div class="row">
        <div class="col-sm-4">
          <h1 class="feedback-form-title">Send us feedback</h1>
          <form class="feedback-form"  method="post" action="/feedback">
            <div class="form-group">
              <label for="feedback-form-name">Name</label>
              <input type="text" name="user[name]" class="form-control" id="feedback-form-name" placeholder="Enter your name">
            </div>

            <div class="form-group">
              <label for="feedback-form-title">Title</label>
              <input type="text" name="user[title]" class="form-control" id="feedback-form-title" placeholder="Title of your feedback">
            </div>

            <div class="form-group">
                <label for="feedback-form-message">Message</label>
                <textarea type="text" name="user[message]" placeholder="Enter your message, then hit the submit"  class="form-control" id="feedback-form-message" rows="6"></textarea>
            </div>
            <button type="submit" class="btn btn-default pull-right">Submit</button>
          </form>
        </div><!-- primary -->
        <aside class="col-sm-8">
          <div class="maincontent feedback">
            
              <h2 class="article-title">Recent Feedback</h2>
              <div class="feedback-messages"></div>
             <% temp.forEach(function(item){ %>
                   <div class="feedback-item item-list media-list">
            <div class="feedback-item media">
              <div class="feedback-info media-body">
                <div class="feedback-head">
                  <small class="feedback-name label label-info"><%=item.name%></small>
                  <div class="feedback-title">  <%=item.title%>  </div>
                </div>
                <div class="feedback-message"><%=item.message%> </div>
              </div>
            </div>
          </div>
             
               <%    }); %>
              
             


          </div>
        </aside><!-- secondary -->
      </div>
    </div><!-- container -->
  <% include partials/template/jsdefaults.ejs %>
  </body>
</html>
